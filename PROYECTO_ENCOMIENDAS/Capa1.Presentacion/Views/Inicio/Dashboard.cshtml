    </div>

    <!-- Modal -->
    <div class="modal fade text-left" id="modalAntecendentePersonal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel19" aria-hidden="true">
        <div class="modal-dialog modal-sm" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="myModalLabel19">Antecedente Personal</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="projectinput1">Tipo de Antecedente (*)</label>
                                <select class="form-control" v-model="AntecedentePersonal.TipoAntecedenteID">
                                    <option value="1">TIPO 1</option>
                                    <option value="2">TIPO 2</option>
                                    @*<option v-for="li in comboCargo" v-text="li.Nombre" :value="li.CargoID"></option>*@
                                </select>
                                @*<small v-if="ifCargo" class="text-danger"> Complete este campo. </small>*@
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="projectinput1">Antecedente Personal (*)</label>
                                <textarea class="form-control" v-model="AntecedentePersonal.Descripcion"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-blue" v-on:click="guardarAntecedentePersonal()">Guardar</button>
                    <button type="button" class="btn grey btn-outline-danger" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>



